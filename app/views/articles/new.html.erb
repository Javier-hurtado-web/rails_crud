<h1>zdarecek</h1>


<% if @article.errors.any? %>
    <% @article.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
<% end %>
<%= form_for @article, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <%= f.text_field :title %>
  <%= f.text_area :body, size: "60x12" %>
  <%= f.submit "Create" %>
<% end %>


